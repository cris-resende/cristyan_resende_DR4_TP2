@page
@model cristyan_resende_DR4_TP2.Pages.CityManager.CityListModel
@{
    ViewData["Title"] = "Lista de Cidades";
}

<h2>@ViewData["Title"]</h2>

<ul>
    @foreach (var city in Model.Cities)
    {
        <li>
            <a asp-page="/CityManager/CityDetails" asp-route-cityName="@city">
                Ver detalhes de @city
            </a>
        </li>
    }
</ul>
